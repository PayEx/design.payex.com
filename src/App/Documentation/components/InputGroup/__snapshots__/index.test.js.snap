exports[`Documentation: InputGroup ValidationStates renders 1`] = `
<Fragment>
  <h2
    id="validation-states"
  >
    Validation states
  </h2>
  <p>
    You can also visualize validation on your input-groups. Just add 
    <Property
      value=".has-success"
    />
     or 
    <Property
      value=".has-error"
    />
     with a 
    <Property
      value=".input-group"
    />
    .
  </p>
  <ComponentPreview
    codeFigure={true}
    language="html"
    removeOuterTag={true}
    showCasePanel={true}
  >
    <InputGroup
      defaultValue="Anderson"
      placeholder="Name"
      prefixValue="Mr."
      type="text"
      validationState="success"
    />
     
    


    <InputGroup
      defaultValue="Smith"
      placeholder="Name"
      prefixValue="Agent"
      type="text"
      validationState="error"
    />
  </ComponentPreview>
</Fragment>
`;

